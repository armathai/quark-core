    "files": [
        "dist/**/*"
    ],
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "esbuild": "^0.12.25",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.1.0",
        "prettier": "^2.3.2",
        "rollup": "^2.56.3",
        "rollup-plugin-dts": "^4.0.0",
        "rollup-plugin-esbuild": "^4.5.0",
        "ts-jest": "^27.0.5",
